begin simulation;    
  time_stop    [time stop] ;
  solvertype   1 ;    (newmark)
  on_no_convergence continue ;
  logfile ../output/log/[Case id.].log ;
;  animation ../output/anim/[Case id.].dat;
  begin newmark;
    deltat    [deltat];  
  end newmark;
end simulation;
