  begin main_body;        
    name        tower ;            
    type        timoschenko ;
    nbodies     1 ;
    node_distribution     c2_def ;
;    damping_posdef   6.456E-4 6.45E-4 1.25E-3 1.4E-3 1.4E-3 1.25E-3 ; Mx My Mz Kx Ky Kz  M압 raises overall level  K압 raises high freguency level
    damping_posdef   0.0 0.0 0.0 1.2E-3 1.2E-3 1.2E-3 ; Mx My Mz Kx Ky Kz   M압 raises overall level K압 raises high freguency level
    gravity 9.81;      
    begin timoschenko_input;
      filename ./data/ny2_tower_st.txt ;
      set 1 1 ; 
    end timoschenko_input;
    begin c2_def;              Definition of centerline (main_body coordinates)
      nsec 8;
      sec 1  0.0 0.0  0.0      0.0 ;  x y z twist
      sec 2  0.0 0.0 -10.0      0.0 ;
